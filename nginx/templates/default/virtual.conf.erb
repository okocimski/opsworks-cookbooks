#
# A virtual host using mix of IP-, name-, and port-based configuration
#

server {
    listen       80;
    server_name  www1.okocimski.net;

    location / {
        proxy_pass http://127.0.0.1:8080;
    }
}


server {
    listen       80;
    server_name  www2.okocimski.net;

    location / {
        proxy_pass http://127.0.0.1:8081;
    }
}
